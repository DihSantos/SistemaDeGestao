@model ConcessionariasModel
﻿@{
    ViewData["Title"] = "Confirmação de exclusão de dados";
}

<div class="text-center">
    <h3 style="color:#585858">Realmente deseja apagar a Concessionária (@Model.Concessionaria)</h3>
</div>

<br />

<a class="btn btn-success" role="button" asp-route-id="@Model.Id" asp-controller="Concessionarias" asp-action="Deletar">Confirmar</a>
<a class="btn btn-primary" role="button" asp-controller="Concessionarias" asp-action="Index">Cancelar</a>